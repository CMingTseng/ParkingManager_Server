<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sunset.model">
	<class name="Portal" table="T_PORTAL" lazy="false">
		<id name="portalId" type="java.lang.String" column="portalId">
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="parkId" column="parkId" type="java.lang.String" />
 
	</class>
</hibernate-mapping>