<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sunset.model">
	<class name="Department" table="T_DEPARTMENT" lazy="false">
		<id name="departmentId" type="java.lang.String" column="departmentId">
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="parentId" column="parentId" type="java.lang.String" />
		<property name="telephone" column="telephone" type="java.lang.String" />
		<property name="fax" column="fax" type="java.lang.String" />
		<property name="zip" column="zip" type="java.lang.String" />
		<property name="address" column="address" type="java.lang.String" />
		<property name="description" column="description" type="java.lang.String" />
		 
	</class>
 
</hibernate-mapping>